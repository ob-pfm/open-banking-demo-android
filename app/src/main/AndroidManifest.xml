<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.openbanking.pfm.sdk.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.OpenBankingDemo">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.openbanking.pfm.sdk.demo.credits.CreditsActivity"
            android:exported="false" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.consent.ConsentDetailActivity"
            android:exported="false" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.consent.ConsentActivity"
            android:exported="true" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.users.UsersActivity"
            android:exported="true" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.transactions.TransactionsActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.transactions.CreateUpdateTransactionActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.accounts.AccountsActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.accounts.CreateUpdateAccountActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.analysis.AnalysisActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.budgets.BudgetsActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.budgets.CreateUpdateBudgetsActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.summary.SummaryActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.categories.CategoriesActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.openbanking.pfm.sdk.demo.categories.CreateUpdateCategoryActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>